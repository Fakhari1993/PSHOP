                         PWHRS_1_1399.dbo.table_003_SubsidiaryGroup ON 
                         PWHRS_1_1399.dbo.table_004_CommodityAndIngredients.column04 = PWHRS_1_1399.dbo.table_003_SubsidiaryGroup.columnid
GROUP BY PWHRS_1_1399.dbo.table_002_MainGroup.column02, PWHRS_1_1399.dbo.table_004_CommodityAndIngredients.column02, 
                         PWHRS_1_1399.dbo.table_004_CommodityAndIngredients.column01, PWHRS_1_1399.dbo.table_004_CommodityAndIngredients.Column35, 
